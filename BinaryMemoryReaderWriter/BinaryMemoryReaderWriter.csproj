<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>BinaryMemoryReaderWriter</AssemblyName>
    <RootNamespace>SharpFast.BinaryMemoryReaderWriter</RootNamespace>
    <Authors>Matthias Lukaseder</Authors>
    <Company>Lukaseder</Company>
    <Description>An optimized binary memory reader and writer.</Description>
    <Copyright>Matthias Lukaseder</Copyright>
    <PackageProjectUrl>https://github.com/GhostTyper/BinaryMemoryReaderWriter</PackageProjectUrl>
    <RepositoryUrl>https://github.com/GhostTyper/BinaryMemoryReaderWriter.git</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>Memory</PackageTags>
    <AssemblyVersion>0.9.9.0</AssemblyVersion>
    <FileVersion>0.9.9.0</FileVersion>
    <Version>0.9.9</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DocumentationFile>..\BinaryMemoryReaderWriter.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DocumentationFile>..\BinaryMemoryReaderWriter.xml</DocumentationFile>
  </PropertyGroup>

</Project>
